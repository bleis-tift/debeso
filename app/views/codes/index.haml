.row.well
  %ul
    - @snippets.each do |snippet|
      %li
        %a{:href => url_for(:codes, :edit, :id => snippet.hash)}= snippet.file_name
  - form_tag url_for(:codes, :create_repository), :method => 'post' do
    = text_field_tag 'snippet_name'
    = submit_tag 'add file', :class => 'btn primary'
